{
	"comments": {
			"lineComment": "--",
			"blockComment": [ "--[[", "]]" ]
	},
	"brackets": [
			["{", "}"],
			["[", "]"],
			["(", ")"]
	],
	"autoClosingPairs": [
			["{", "}"],
			["[", "]"],
			["(", ")"],
			["\"", "\""],
			["'", "'"],
			["|", "|"]
	],
	"surroundingPairs": [
			["{", "}"],
			["[", "]"],
			["(", ")"],
			["\"", "\""],
			["'", "'"],
			["|", "|"]
	],
	"indentationRules": {
			"increaseIndentPattern": "^((?!(\\-\\-)).)*((\\b((class\\s+.+)|(?=(constructor\\s*\\(.*\\)))|else|function|then|do|repeat)\\b((?!\\b(end|until)\\b).)*)|(\\{\\s*))$",
			"decreaseIndentPattern": "^\\s*((\\b(elseif|else|end|until)\\b)|(\\})|(\\)))"
	}
}
